<h2>Publishers</h2>

<table class="table table-striped">
  <tr>
    <th>Publishers Name</th>
    <th>Actions</th>
  <tr>
  <% @publisher.each do |publisher| %>
    <tr>
      <td><%= publisher.name %></td>
      <td><%= link_to 'Edit',   edit_publisher_path(publisher.id)%>  |
          <%= link_to 'Delete', publisher_path(publisher) ,
          method: :delete, data: {confirm: 'Are you sure?'} %> </td>
    </tr>
  <%end%>
</table>
